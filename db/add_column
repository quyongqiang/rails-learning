rails g migration add_email_to_users

# 2. change the migrate file
class AddEmailToUsers < ActiveRecord::Migration
  def change
    add_column :users, :email, :string, :null => false, :default => ""
  end
end


# 3. exec the migrate
rake db:migrate

# 4. load the fixtures' data
rake db:fixtures:load
